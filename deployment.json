{
    "priority": 100,
    "content": {
        "modulesContent": {
            "$edgeAgent": {
                "properties.desired": {
                    "schemaVersion": "1.1",
                    "runtime": {
                        "type": "docker",
                        "settings": {
                            "registryCredentials": {
                                "cricarusprod001": {
                                    "address": "{{CONTAINER_REGISTRY_URL}}",
                                    "username": "{{CONTAINER_REGISTRY_USERNAME}}",
                                    "password": "{{CONTAINER_REGISTRY_PASSWORD}}"
                                }
                            }
                        }
                    },
                    "systemModules": {
                        "edgeAgent": {
                            "settings": {
                                "image": "mcr.microsoft.com/azureiotedge-agent:1.4"
                            },
                            "type": "docker"
                        },
                        "edgeHub": {
                            "restartPolicy": "always",
                            "settings": {
                                "image": "mcr.microsoft.com/azureiotedge-hub:1.4",
                                "createOptions": "{\"HostConfig\":{\"PortBindings\":{\"443/tcp\":[{\"HostPort\":\"443\"}],\"5671/tcp\":[{\"HostPort\":\"5671\"}],\"8883/tcp\":[{\"HostPort\":\"8883\"}]}}}"
                            },
                            "status": "running",
                            "type": "docker"
                        }
                    },
                    "modules": {
                        "icarus-edge-redis": {
                            "imagePullPolicy": "on-create",
                            "restartPolicy": "always",
                            "settings": {
                                "image": "cricarusprod001.azurecr.io/icarus-edge-redis:{{BUILD_VERSION}}"
                            },
                            "status": "running",
                            "type": "docker"
                        },
                        "icarus-edge-detector": {
                            "restartPolicy": "always",
                            "env": {
                                "VIDEO_OUTPUT_HEIGHT": {
                                    "value": "1080"
                                },
                                "VIDEO_OUTPUT_WIDTH": {
                                    "value": "1920"
                                },
                                "VIDEO_OUTPUT_FRAMERATE": {
                                    "value": "30"
                                }
                            },
                            "settings": {
                                "image": "cricarusprod001.azurecr.io/icarus-edge-detector:{{BUILD_VERSION}}",
                                "createOptions": "{\"HostConfig\":{\"Privileged\":true,\"Binds\":[\"/dev/shm:/dev/shm\"],\"shmSize\":500000},\"Entrypoint\":[\"/home/docker_user/scripts/start.dev.sh\",\"yolov5n\"]}"
                            },
                            "status": "running",
                            "type": "docker"
                        },
                        "icarus-edge-sender": {
                            "restartPolicy": "always",
                            "settings": {
                                "image": "cricarusprod001.azurecr.io/icarus-edge-sender:{{BUILD_VERSION}}",
                                "createOptions": "{\"HostConfig\":{\"Binds\":[\"/dev/shm:/dev/shm\"],\"shmSize\":500000},\"Entrypoint\":[\"/home/docker_user/scripts/start.dev.sh\"]}"
                            },
                            "env": {
                                "REMOTE_IP_ADDRESS": {
                                    "value": "{{REMOTE_IP_ADDRESS}}"
                                },
                                "REMOTE_PORT": {
                                    "value": "{{REMOTE_PORT}}"
                                },
                                "REMOTE_RABBITMQ_PORT": {
                                    "value": "{{REMOTE_RABBITMQ_PORT}}"
                                },
                                "REMOTE_RABBITMQ_USERNAME": {
                                    "value": "{{REMOTE_RABBITMQ_USERNAME}}"
                                },
                                "REMOTE_RABBITMQ_PASSWORD": {
                                    "value": "{{REMOTE_RABBITMQ_PASSWORD}}"
                                },
                                "REDIS_IP_ADDRESS": {
                                    "value": "{{REDIS_IP_ADDRESS}}"
                                }
                            },
                            "status": "running",
                            "type": "docker"
                        }
                    }
                }
            },
            "$edgeHub": {
                "properties.desired": {
                    "schemaVersion": "1.1",
                    "storeAndForwardConfiguration": {
                        "timeToLiveSecs": 7200
                    },
                    "routes": {}
                }
            }
        }
    },
    "metrics": {
        "queries": {}
    },
    "labels": {},
    "etag": "",
    "id": "ssss"
}